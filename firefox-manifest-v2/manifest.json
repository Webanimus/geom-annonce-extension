{
    "manifest_version": 2,
    "name": "Geom-annonce",
    "description": "Choisissez une annonce en ligne pour obenir l'adresse du bien immobilier présenté.",
    "version": "1.0.1",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Copier l'annonce",
        "default_icon": {
            "128": "icon.png"
        }
    },
    "permissions": [
        "activeTab",
        "clipboardWrite"
    ]
}