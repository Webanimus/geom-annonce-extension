{
    "manifest_version": 3,
    "name": "GeomAnnonce",
    "version": "1.0.2",
    "description": "Obtenez l'adresse exacte des annonces immobili√®res en ligne.",
    "action": {
      "default_icon": {
        "128": "icon.png"
      },
      "default_popup": ""
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["clipboardWrite", "activeTab"],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["axios.min.js", "content.js"]
    }]
  }