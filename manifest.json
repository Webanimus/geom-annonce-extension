{
  "manifest_version": 3,
  "name": "Geom Annonce",
  "version": "1.1.5",
  "description": "Trouvez l'adresse exacte des annonces immobili√®res en ligne.",
  "action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.leboncoin.fr/*",
        "https://*.leboncoin.fr/*",
        "http://*.seloger.com/*",
        "https://*.seloger.com/*"
      ],
      "js": [
        "env.js",
        "axios.min.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
